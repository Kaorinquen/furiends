<header>
        <nav class="navbar navbar-light bg-primary">
          <div class="navbar navbar-light bg-light">
            <a class="navbar-brand" href="#">
              <img src="/docs/4.3/assets/brand/bootstrap-solid.svg" width="30" height="30" class="d-inline-block align-top"
                alt="">
              Welcome <%= user.userName %>
            </a>
          </div>
          <ul class="nav justify-content-end">
            <li class="nav-item">
              <a href="/users/logout" class="btn btn-success" id="log">Logout</a>
            </li>
          </ul>
        </nav>
      </header>
      <div class="clear"></div>
      <div class="container">
        <div class="jumbotron" id=userInfo>
          <div id="wrapper">
            <h1 class="display-4">Hello, world!</h1>
            <img src="#" alt="#">
            <div id=></div>
            <div id="userBio">Bio</div><button type="button" data-toggle=button> </button>
            <div class="profileTextStyle" id="userName"></div><button type="button" data-toggle=button id="userNameEdit">Edit
            </button>
            <div class="profileTextStyle" id="userSpecies"></div><button type="button" data-toggle=button id="spcEdit">Edit
            </button>
            <div class="profileTextStyle" id="userBreed"></div> <button type="button" data-toggle=button id="brdEdit">Edit
            </button>
            <div class="profileTextStyle" id="userLocation"></div> <button type="button" data-toggle=button id="locEdit">Edit
            </button>
            <div>
              <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#uploadModal">
                Upload new image
              </button>
            </div>
          </div>
        </div>
      
        <div class="modal" tabindex="-1" role="dialog" id="uploadModal">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title">Upload an Image</h5>
      
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body">
                <form action="/api/upload" method="post" enctype="multipart/form-data">
                  comment: <input id="comment" type="textarea" name="comment"><br>
                  Your image: <input type="file" name="image"><br>
                  <input type="hidden" name="userName" value="<%= user.userName %>"></input>
                  <input type="hidden" name="id" value="<%= user.id %>"></input>
                  <button type="submit" value="Submit">Upload</button>
                </form>
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
              </div>
            </div>
          </div>
        </div>
      
        <div  id="container">
        </div>
      
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
          <script
            src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
            integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
            crossorigin="anonymous"
          ></script>
          <script
            src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
            integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
            crossorigin="anonymous"
          ></script>
        <script src="js/index.js"></script>